set(feelpp-core_VERSION @feelpp-core_VERSION@)

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include(GNUInstallDirs)
include(FindPackageHandleStandardArgs)


set( FEELPP_CORE_LIBRARY @FEELPP_CORE_LIBRARY@ )
set( FEELPP_CORE_LIBRARY_PREFIX @FEELPP_CORE_LIBRARY_PREFIX@ )
set( FEELPP_CORE_CMAKE_PREFIX @FEELPP_CORE_CMAKE_PREFIX@ )

set_and_check(FEELPP_CORE_CONFIG_INSTALL_DIR "@PACKAGE_FEELPP_CORE_CONFIG_INSTALL_DIR@")
set_and_check(FEELPP_CORE_DATAROOTDIR_INSTALL_DIR "@PACKAGE_FEELPP_CORE_DATAROOTDIR_INSTALL_DIR@")

set( FEELPP_CORE_CMAKE_DIR ${FEELPP_CORE_DATAROOTDIR_INSTALL_DIR}/cmake )

include(${FEELPP_CORE_CMAKE_DIR}/macro.cmake)
include(${FEELPP_CORE_CMAKE_DIR}/importDependenciesHelpers.cmake)
include(${FEELPP_CORE_CMAKE_DIR}/importDependenciesPackageConfig.cmake)
importDependenciesPackageConfig( ${FEELPP_CORE_LIBRARY} ${FEELPP_CORE_LIBRARY_PREFIX} ${FEELPP_CORE_CONFIG_INSTALL_DIR} )

set(${CMAKE_FIND_PACKAGE_NAME}_CONFIG ${CMAKE_CURRENT_LIST_FILE})
find_package_handle_standard_args( feelpp-core REQUIRED_VARS FEELPP_CORE_CONFIG_INSTALL_DIR CONFIG_MODE )
