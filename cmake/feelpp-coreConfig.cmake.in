set(ktirio-geom_VERSION @ktirio-geom_VERSION@)

@PACKAGE_INIT@

set( KTIRIO_GEOM_LIBRARY @KTIRIO_GEOM_LIBRARY@ )
set( KTIRIO_GEOM_LIBRARY_PREFIX @KTIRIO_GEOM_LIBRARY_PREFIX@ )
set( KTIRIO_GEOM_CMAKE_PREFIX @KTIRIO_GEOM_CMAKE_PREFIX@ )

set_and_check(KTIRIO_GEOM_CONFIG_INSTALL_DIR "@PACKAGE_KTIRIO_GEOM_CONFIG_INSTALL_DIR@")
include(${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/macro.cmake)
include(CMakeFindDependencyMacro)
include(GNUInstallDirs)
include (FindPackageHandleStandardArgs)

set(${CMAKE_FIND_PACKAGE_NAME}_CONFIG ${CMAKE_CURRENT_LIST_FILE})
find_package_handle_standard_args( ktirio-geom REQUIRED_VARS KTIRIO_GEOM_CONFIG_INSTALL_DIR CONFIG_MODE)


if ( NOT TARGET Ktirio-Geom::${KTIRIO_GEOM_LIBRARY} )
  include(${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/${KTIRIO_GEOM_LIBRARY_PREFIX}_third_party_definitions-targets.cmake)
  feelpp_set_options( Ktirio-Geom::${KTIRIO_GEOM_LIBRARY_PREFIX}_third_party_definitions ${KTIRIO_GEOM_CMAKE_PREFIX} "HAS" )
  feelpp_set_options( Ktirio-Geom::${KTIRIO_GEOM_LIBRARY_PREFIX}_third_party_definitions ${KTIRIO_GEOM_CMAKE_PREFIX} "USE_INTERNAL" )

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_NAPP )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_NAPP )
      find_dependency(napp PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../napp NO_DEFAULT_PATH )
    else()
      find_dependency(napp)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_FMT )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_FMT )
      find_dependency(fmt PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../fmt NO_DEFAULT_PATH )
    else()
      find_dependency(fmt)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_BOOST )
    set(BOOST_MIN_VERSION @BOOST_MIN_VERSION@)
    set(BOOST_COMPONENTS @BOOST_COMPONENTS@)
    find_dependency(Boost ${BOOST_MIN_VERSION} REQUIRED COMPONENTS ${BOOST_COMPONENTS})
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_NLOHMANN_JSON )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_NLOHMANN_JSON )
      find_dependency(nlohmann_json PATHS ${PACKAGE_PREFIX_DIR}/${CMAKE_INSTALL_DATADIR}/cmake/nlohmann_json NO_DEFAULT_PATH )
    else()
      find_dependency(nlohmann_json)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_SPDLOG )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_SPDLOG )
      find_dependency(spdlog PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../spdlog NO_DEFAULT_PATH )
    else()
      find_dependency(spdlog)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_LIBASSERT )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_LIBASSERT )
      find_dependency(libdwarf PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../libdwarf NO_DEFAULT_PATH )
      find_dependency(cpptrace PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../cpptrace NO_DEFAULT_PATH )
      find_dependency(libassert PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../libassert NO_DEFAULT_PATH )
    else()
      find_dependency(libassert)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_CPR )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_CPR )
      find_dependency(cpr PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../cpr NO_DEFAULT_PATH )
    else()
      find_dependency(cpr)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_EIGEN3 )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_EIGEN3 )
      find_dependency(Eigen3 PATHS ${PACKAGE_PREFIX_DIR}/${CMAKE_INSTALL_DATADIR}/eigen3/cmake NO_DEFAULT_PATH )
    else()
      find_dependency(Eigen3)
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_BVH )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_BVH )
      include(${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../bvh/bvhTargets.cmake )
    else()
    endif()
  endif()

  if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_HAS_CGAL )
    if ( ${KTIRIO_GEOM_CMAKE_PREFIX}_USE_INTERNAL_CGAL )
      find_dependency(CGAL COMPONENTS Core PATHS ${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/../CGAL NO_DEFAULT_PATH )
    else()
      find_dependency(CGAL COMPONENTS Core )
    endif()
    include(CGAL_Eigen3_support)
  endif()

  include(${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/${KTIRIO_GEOM_LIBRARY_PREFIX}_third_party_dependencies-targets.cmake)
  include(${KTIRIO_GEOM_CONFIG_INSTALL_DIR}/${KTIRIO_GEOM_LIBRARY}-targets.cmake)
endif()
